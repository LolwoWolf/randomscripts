Weapons:addButton("Buy Beretta M9 ($900)", function()
	local OLDPOS = plr.Character.HumanoidRootPart.CFrame

	wait(0.5)

	plr.Character.HumanoidRootPart.CFrame = CFrame.new(-1223, 24, -192)

	wait(0.5)

	-- BuyGun Variable

	local args = {
		[1] = "Beretta M9"
	}

	game:GetService("ReplicatedStorage").FE.BuyGun:InvokeServer(unpack(args))

	-- EquipGun Variable

	local args = {
		[1] = "Beretta M9",
		[2] = true
	}

	game:GetService("ReplicatedStorage").FE.EquipGun:InvokeServer(unpack(args))

	wait(0.3)

	plr.Character.HumanoidRootPart.CFrame = OLDPOS
end)

