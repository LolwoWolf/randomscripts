
-- Library Loader

local library = loadstring(game:HttpGet("https://raw.githubusercontent.com/GreenDeno/Venyx-UI-Library/main/source.lua"))()
local venyx = library.new("Random UI For ERLC", 5013109572)

-- Themes

local themes = {
	Background = Color3.fromRGB(24, 24, 24),
	Glow = Color3.fromRGB(0, 0, 0),
	Accent = Color3.fromRGB(10, 10, 10),
	LightContrast = Color3.fromRGB(20, 20, 20),
	DarkContrast = Color3.fromRGB(14, 14, 14),  
	TextColor = Color3.fromRGB(255, 255, 255)
}

-- Pages

local MainPage = venyx:addPage("Main", 5012544693)
local CrimPage = venyx:addPage("Criminal", 5012544693)
local Weapons = venyx:addPage("Weapons", 5012544693)
local Vehicle = venyx:addPage("Vehicle", 5012544693)
local TPPage = venyx:addPage("Teleports", 5012544693)

-- Main Tabs

local Main = MainPage:addSection("Main")


-- Teleport Tabs

local TeleportsShops = TPPage:addSection("Main TPs")
local TeleportsMisc = TPPage:addSection("Misc TPs")

-- Criminal Tabs

local Criminal = CrimPage:addSection("Criminal")
local CriminalTps = CrimPage:addSection("Criminal TPs")
local VehicleTabs = Vehicle:addSection("Vehicle")

-- Weapons Tabs

local WeaponsMods = Weapons:addSection("Weapon Mods")
local Weapons = Weapons:addSection("Buy Weapons")

-- Main


Main:addButton("Refill Stamina", function()
	local Stamina = game:GetService("Players").LocalPlayer.PlayerGui.GameGui.BottomLeft.Health["Stamina LS"].Stamina
	Stamina.Value = 100
end)

Main:addToggle("Infinite Stamina (cant be disabled)", nil, function(stamina)
	while stamina == true do
		local Stamina = game:GetService("Players").LocalPlayer.PlayerGui.GameGui.BottomLeft.Health["Stamina LS"].Stamina
		Stamina.Value = 100
		wait(2.5)
	end
end)

Main:addButton("Change To Police (wip)", function()
	--LoadMenu

	local args = {
		[1] = "Civilian",
		[2] = "MenuHint"
	}

	game:GetService("ReplicatedStorage").FE.NewPlayerTasks:FireServer(unpack(args))

	--CanChangeTeam

	local args = {
		[1] = BrickColor.new(102)
	}

	game:GetService("ReplicatedStorage").FE.CanChangeTeam:InvokeServer(unpack(args))

	--CheckCallSign

	local args = {
		[1] = "1137"
	}

	game:GetService("ReplicatedStorage").FE.CheckCallsign:InvokeServer(unpack(args))

	--TeamChange

	local args = {
		[1] = BrickColor.new(102),
		[2] = "1137"
	}

	game:GetService("ReplicatedStorage").FE.TeamChange:FireServer(unpack(args))
end)

Main:addButton("Change To Civilian", function()

	--NewPlayerTasks

	local args = {
		[1] = "Civilian",
		[2] = "MenuHint"
	}

	game:GetService("ReplicatedStorage").FE.NewPlayerTasks:FireServer(unpack(args))

	--TeamChange

	local args = {
		[1] = BrickColor.new(311)
	}

	game:GetService("ReplicatedStorage").FE.TeamChange:FireServer(unpack(args))
end)

-- Criminal
local plr = game:GetService('Players').LocalPlayer

Criminal:addButton("Buy All Tools", function()
	local OLDPOS = plr.Character.HumanoidRootPart.CFrame
	
	wait(0.3)
	
	plr.Character.HumanoidRootPart.CFrame = CFrame.new(-437, 25, -711)
	
	wait(0.3)

	local args = {
		[1] = "Lockpick"
	}

	game:GetService("ReplicatedStorage").FE.BuyGear:InvokeServer(unpack(args))

	local args = {
		[1] = "Drill"
	}

	game:GetService("ReplicatedStorage").FE.BuyGear:InvokeServer(unpack(args))


	local args = {
		[1] = "RFID Disruptor"
	}

	game:GetService("ReplicatedStorage").FE.BuyGear:InvokeServer(unpack(args))
	
	wait(0.2)

	plr.Character.HumanoidRootPart.CFrame = OLDPOS
end)

CriminalTps:addButton("Jewlery Store", function()
	plr.Character.HumanoidRootPart.CFrame = CFrame.new(-463, 23, -400)
end)

CriminalTps:addButton("Bank", function()
	plr.Character.HumanoidRootPart.CFrame = CFrame.new(-1000, 24, 429)
